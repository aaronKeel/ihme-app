{
  "name": "ihme-app",
  "version": "1.0.0",
  "description": "starter app",
  "scripts": {
    "tsc:server:dev": "tsc --watch --project server/tsconfig.json",
    "server:dev": "nodemon ./dist/server/index.js",
    "build:dev": "NODE_ENV=development node_modules/.bin/webpack --colors --config webpack.config.dev.js --watch",
    "tslint:server": "node_modules/.bin/tslint -p ./server/tsconfig.json",
    "tslint:public": "node_modules/.bin/tslint -p ./public/app/tsconfig.json",
    "test:public": "$(npm bin)/_mocha --project public/app/tsconfig.json --compilers ts:ts-node/register,tsx:ts-node/register --require ignore-styles --full-trace `find ./public -name \"test\" -type d`"
  },
  "author": "Aaron Keel",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.0.10",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/express": "^4.11.0",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/sinon": "^4.1.2",
    "autoprefixer": "^7.2.3",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^4.1.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^3.10.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-notifier": "^1.5.0"
  },
