{
  "name": "ihme-app",
  "version": "1.0.0",
  "description": "starter app",
  "scripts": {
    "tsc:w": "tsc --watch",
    "server:dev": "nodemon ./dist/index.js",
    "build:dev": "NODE_ENV=development node_modules/.bin/webpack --colors --config webpack.config.dev.js --watch",
    "docker:build:dev": "docker build -t starter-app -f ./Docker/dev/Dockerfile .",
    "docker:run": "docker run --rm --publish 8080:8080 --name starter-app -v `pwd`:/var/www/html/starter-app -e ENVIRONMENT_NAME='dev' starter-app"
  },
  "author": "Aaron Keel",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.11.0",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "autoprefixer": "^7.2.3",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^3.10.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-notifier": "^1.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.14.3"
  }
}
